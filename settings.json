{
  "editor.fontSize": 14,
  "workbench.activityBar.visible": true,
  "editor.renderWhitespace": "boundary",
  "editor.minimap.enabled": false,
  "editor.tabSize": 2,
  "eslint.autoFixOnSave": true,
  "eslint.validate": [
    {
      "language": "vue",
      "autoFix": true
    },
    {
      "language": "javascript",
      "autoFix": true
    },
  ],
  "emmet.triggerExpansionOnTab": true,
}
